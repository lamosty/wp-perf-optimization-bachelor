  \printfield{pagetotal}%
  \space\newblock  
  \usebibmacro{series+number}.
  \space\newblock  
  \iftoggle{bbx:isbn}
    {\printfield{isbn}}
    {}%
  \newunit\newblock
  \usebibmacro{addendum+pubstate}%
